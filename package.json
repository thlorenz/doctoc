{
  "author": "Thorsten Lorenz <thlorenz@gmx.de> (thlorenz.com)",
  "name": "doctoc",
  "description": "Generates TOC for markdown files of local git repo.",
  "version": "2.0.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/thlorenz/doctoc.git"
  },
  "scripts": {
