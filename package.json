{
  "author": "Thorsten Lorenz <thlorenz@gmx.de> (thlorenz.com)",
  "name": "doctoc",
  "description": "Generates TOC for markdown files of local git repo.",
  "version": "1.1.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/thlorenz/doctoc.git"
  },
  "scripts": {
    "test": "set -e; for t in test/*.js; do node $t; done"
  },
  "main": "doctoc.js",
  "bin": "doctoc.js",
  "engines": {
    "node": ">=0.4"
  },
  "dependencies": {
    "anchor-markdown-header": "^0.5.4",
    "htmlparser2": "~3.7.1",
    "markdown-to-ast": "~3.2.3",
    "minimist": "~1.1.0",
    "underscore": ">=1.3.3",
    "update-section": "^0.3.0"
  },
  "devDependencies": {
    "tap": "~0.4.8"
  },
  "license": "MIT",
  "keywords": [
    "github",
    "markdown",
    "documentation",
    "readme",
    "parser",
    "bitbucket",
    "gitlab",
    "ghost"
  ]
}
